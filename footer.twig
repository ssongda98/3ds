<div class="bg-white text-black flex flex-wrap overflow-hidden">
    <div class="splide2">
        <div class="splide__track">
            <ul class="splide__list">
                {% for i in options.logos %}
                    <li class="splide__slide flex justify-center flex-col">
                    <span class="p-4"><img class="flex-none grayscale" src="{{i.image|resize(140)}}" /></span></li>
                {% endfor %}   
            </ul>
        </div>
    </div>
</div>

<div id="footer" class="bg-cover p-10 lg:p-20" style="background-image: url('{{site.theme.link}}/assets/img/footer.jpg');">
    <div class="flex flex-col lg:flex-row">
        <div class="flex flex-col lg:w-4/12 lg:pr-20">
            <a class="inline-block" href="{{site.url}}" rel="home"><span class="screen-reader-text">{{site.name}}</span> <img class="w-[350px]" src="{{site.theme.link}}/assets/img/logo.png" /></a>

            <hr class="c-footer-hr relative border-0 overflow-visible mt-10 mb-20   ">

            <h3 class="text-xl c-footer__title relative pb-2 mb-10">Services</h3>
            <ul>
                {% for item in footerServicesMenu.get_items %}
                    <li class="{{ item.classes | join(' ') }}">
                        <a target="{{ item.target }}" href="{{ item.link }}" class="hover:text-yellow transition">{{ item.title }}</a>
                    </li>
                {% endfor %}
            </ul>

            <h3 class="text-xl c-footer__title relative pb-2 mb-10 mt-10">Connect</h3>

